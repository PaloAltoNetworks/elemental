                {{- end }}
                {{- if gt (len $action.RequiredParameters) 0 }}
                RequiredParameters: elemental.NewParametersRequirement(
                    [][][]string{
                        {{- range $i, $l1 := $action.RequiredParameters }}
                        {{- if gt (len $l1) 0 }}
                        [][]string{
                            {{- range $i, $l2 := $l1 }}
                            {{- if gt (len $l2) 0 }}
                            []string{
                                {{- range $i, $l3 := $l2 }}
